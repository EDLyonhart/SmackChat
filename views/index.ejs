<h1>Login / Signup Page</h1>

<form action="/create" method="POST">
  <p>
    <label for="signUp">SignUp: </label>
    <input id="signUp" type="text" name="signUp" required>
    <label for="password">Password: </label>
    <input id="password" type="password" name="signUp" required>
    
    <input type="submit" value="Create">
  </p> 
</form>

<form>
  <p>
    <label for="userName">UserName: </label>
    <input id="userName" type="text" name="signIn" required>
    <label for="userPass">Password: </label>
    <input id="userPass" type="password" name="userPass">

    <input type="submit" value="Login">
  </p>
</form>